public class botonPiso extends boton {
    private String direccion;

    public botonPiso(String direccion) {
        this.direccion = direccion;
    }

    public void enviarSolicitud(SistemaControl sistema, int piso, Modulonotificaciones notificaciones) {
        presionar(notificaciones);
        sistema.recibirSolicitud(piso, direccion);
    }
}
